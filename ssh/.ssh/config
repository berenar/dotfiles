# Added by OrbStack: 'orb' SSH host for Linux machines
# This only works if it's at the top of ssh_config (before any Host blocks).
# This won't be added again if you remove it.
Include ~/.orbstack/ssh/config

Host macmini
  HostName 192.168.10.240
  User runner
  PubkeyAuthentication no
  PasswordAuthentication yes
  
Host github.com
  HostName github.com
	UseKeychain yes
  IdentityFile ~/.ssh/github

Host gitlab.com
  AddKeysToAgent yes
  UseKeychain yes
  Hostname gitlab.com
  IdentityFile ~/.ssh/gitlab

Host bitbucket.org
	AddKeysToAgent yes
	UseKeychain yes
  Hostname bitbucket.org
  IdentityFile ~/.ssh/clbrt-bitbucket

Host raspberri
  User berenar
  Hostname raspberrypizero2w.local
  UseKeychain yes
  IdentityFile /Users/berenar/.ssh/raspberri.pem

